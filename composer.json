{
  "name": "phoole/env",
  "type": "library",
  "license": "Apache-2.0",
  "version": "1.1.0",
  "description": "Simple environment variables loader library for PHP",
  "keywords": [
    "phoole",
    "library",
    "php",
    "framework"
  ],
  "authors": [
    {
      "name": "Hong Zhang",
      "homepage": "https://github.com/phoole"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "phoole/base": "~1.0.18"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "Phoole\\Env\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phoole\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check": "phpcs --standard=psr12 src/",
    "format": "phpcbf --standard=psr12 src/"
  }
}